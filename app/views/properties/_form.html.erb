<%= form_with(model: @property, local: true) do |form| %>
  <div>
    <%= form.label :物件名 %>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :賃料 %>
    <%= form.text_field :rent %>
  </div>
  <div>
    <%= form.label :住所 %>
    <%= form.text_field :address %>
  </div>
  <div>
    <%= form.label :築年数 %>
    <%= form.text_field :age_building %>
  </div>
  <div>
    <%= form.label :備考 %>
    <%= form.text_field :remarks %>
  </div>
<% end %>
<%= link_to "もどる", properties_path %>